package com.sist.di2;

public class Sawon {
	private String name;
	private Sawon(String name) {
		this.name=name;
		System.out.println("생성자를 통해 값을 입력...");
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
		System.out.println("setName()을 통해 값을 입력..");
	}
	
}
