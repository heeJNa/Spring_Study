<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- 
			DI (의존성 주입) : 멤버변수에 값을 주입(초기화), 생성자를 통해서 주입
								메소드 호출이 가능(객체 생성시, 객체 소멸시)
	 -->
	 <bean id="sa" class="com.sist.di.Sawon">
	 	<property name="name">
	 		<value>홍길동</value> <!-- sa.setName("홍길동") -->
	 	</property>
	 	<property name="dept">
	 		<value>개발부</value> <!-- sa.setName("홍길동") -->
	 	</property>
	 	<property name="job">
	 		<value>대리</value> <!-- sa.setName("홍길동") -->
	 	</property>
	 	<property name="age">
	 		<value>25</value> <!-- sa.setName("홍길동") -->
	 	</property>
	 	<property name="pay">
	 		<value>3000</value> <!-- sa.setName("홍길동") -->
	 	</property>
	 </bean>

</beans>
